{
  "name": "Examination-2",
  "version": "0.3.0",
  "description": "Restful API",
  "main": "dist",
  "scripts": {
    "start": "eslint src && nodemon src/server.js --exec babel-node --presets es2015,stage-2",
    "test": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
