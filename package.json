    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Loke Carlsson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/1dv527/lc222ak-examination-2.git"
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "bluebird": "^3.4.7",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "jsonwebtoken": "^7.3.0",
    "mongoose": "^4.8.2",
    "morgan": "^1.8.1",
    "passport": "^0.3.2",
    "passport-jwt": "^2.2.1",
    "passport-local": "^1.0.0",
    "request": "^2.79.0",
    "resource-router-middleware": "^0.6.0"
  },
  "devDependencies": {
    "babel-cli": "^6.22.2",
    "babel-core": "^6.22.1",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.1.1",
    "nodemon": "^1.9.2"
  }
